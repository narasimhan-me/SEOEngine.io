{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "test -n \"$DATABASE_URL\" && prisma generate || echo 'Skipping prisma generate (no DATABASE_URL)'",
    "build": "prisma generate && nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/api/src/main",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:all": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "seed": "prisma db execute --file prisma/seed.sql",
    "seed:ts": "ts-node prisma/seed.ts",
    "test:api": "jest --config jest.config.ts",
    "test:unit": "jest --config jest.unit.config.ts",
    "test:unit:watch": "jest --config jest.unit.config.ts --watch",
    "test:api:critical": "jest --config jest.critical-integration.config.ts",
    "test:api:e2e": "jest --config jest.e2e.config.ts",
    "db:test:migrate": "ts-node scripts/db-test-migrate.ts",
    "db:test:reset": "ts-node scripts/db-test-reset.ts",
    "test:shopify:live": "ENGINEO_LIVE_SHOPIFY_TEST=1 ts-node scripts/shopify-live-smoke.ts",
    "test:shopify:live:dry": "ENGINEO_LIVE_SHOPIFY_TEST=1 ts-node scripts/shopify-live-smoke.ts --dry-run"
  },
  "prisma": {
    "seed": "prisma db execute --file prisma/seed.sql"
  },
  "dependencies": {
    "@engineo/shared": "workspace:*",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^4.1.0",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@types/cheerio": "^1.0.0",
    "stripe": "^14.0.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.0.0",
    "cheerio": "^1.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "ioredis": "^5.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "speakeasy": "^2.0.0",
    "stripe": "^14.25.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.6",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.6",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prisma": "^7.0.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
